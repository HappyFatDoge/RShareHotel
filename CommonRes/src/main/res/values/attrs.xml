<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">
    
    <!--toolbar样式属性-->
    <declare-styleable name="CommonToolBar" tools:ignore="ResourceName">
        <attr name="title" format="string|reference"></attr>
        <attr name="backVisible" format="boolean"></attr>
        <attr name="rightTextViewVisible" format="boolean"></attr>
        <attr name="rightImageViewVisible" format="boolean"></attr>
        <attr name="rightSrcCompat" format="reference"></attr>
        <attr name="rightText" format="string|reference"></attr>
    </declare-styleable>



    <!--提示dialog属性-->
    <declare-styleable name="TipsDialog" tools:ignore="ResourceName">
        <attr name="TipsTitleAppearance" format="reference"></attr>
        <attr name="TipsTitleText" format="reference|string"></attr>
        <attr name="TipsContent" format="reference|string" ></attr>
        <attr name="ConfirmButtonText" format="reference|string"></attr>
    </declare-styleable>

    <declare-styleable name="RoundProgressBar" tools:ignore="ResourceName">
        <attr name="roundColor" format="color" />
        <attr name="roundProgressColor" format="color" />
        <attr name="roundWidth" format="dimension"></attr>
        <attr name="textColor" />
        <attr name="textSize" />
        <attr name="max" format="integer"></attr>
        <attr name="text" format="string"></attr>
        <attr name="textIsDisplayable" format="boolean"></attr>
        <attr name="style">
            <enum name="STROKE" value="0"></enum>
            <enum name="FILL" value="1"></enum>
        </attr>
    </declare-styleable>
</resources>